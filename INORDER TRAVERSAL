class Node:
    def __init__(self, data):
        self.left = None
        self.data= data
        self.right = None


def inorder_traversal(root):
    if root:
        print(root.data, end=" ")
        inorder_traversal(root.left)
        inorder_traversal(root.right)


root = Node(1)
root.left = Node(2)
root.right = Node(3)
root.left.left = Node(4)
root.left.right = Node(5)

print("inorder Traversal:")
inorder_traversal(root)
