def QS(arr):
    if(len(arr)<=1):
        return arr
    pivot=arr[0]
    left=[]
    right=[]
    for i in range(1,len(arr)):
        if(arr[i]<pivot):
            left.append(arr[i])
        else:
            right.append(arr[i])
    return QS(left)+[pivot]+QS(right)


arr=[8,10,22,33,4,45,11,66,12,77,6]
result=QS(arr)
print(result)
