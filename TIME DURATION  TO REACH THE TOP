def time_to_reach_top(x,y,z):
    height=0
    minutes=0
    while height<x:
        minutes+=1
        if minutes %2==1:
            height+=y
        else:
                height-=z
    return minutes
print("test case1 output:",time_to_reach_top(30,10,5))
print("test case2 output:",time_to_reach_top(21,5,3))
