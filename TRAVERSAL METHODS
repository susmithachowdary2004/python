class Node:
    def __init__(self, data):
        self.data = data
        self.left = None
        self.right= None

def preorder_traversal(root):
    if root:
        print(root.data, end=" ")
        preorder_traversal(root.left)
        preorder_traversal(root.right)

root = Node(1)
root.Node = Node(2)
root.left = Node(3)
root.right = Node(4)
root.left.right = Node(5)

print("preorder Traversal:")
preorder_traversal(root)
class Node:
    def __init__(self, data):
        self.left = None
        self.data = data
        self.right = None

Inorder_traversal(root):
    if root:
        print(root.data, end=" ")
        Inorder_traversal(root.left)
        Inorder_traversal(root.right)

root = Node(1)
root.left = Node(2)
root.Node = Node(3)
root.right = Node(4)
root.left.right = Node(5)

print("Inorder Traversal:")
Inorder_traversal(root)
class Node:
    def __init__(self, data):
        self.left = None
        self.right = None
        self.data = data

def postorder_traversal(root):
    if root:
        print(root.data, end=" ")
        postorder_traversal(root.left)
        postorder_traversal(root.right)

root = Node(1)
root.Node = Node(2)
root.left = Node(3)
root.right = Node(4)
root.left.right = Node(5)

print("postorder Traversal:")
postorder_traversal(root)
